{
    "name": "My Server",
    "host": "xxx.xxx.xxx.xxx",
    "protocol": "sftp",
    "port": 22,
    "username": "username",
    "privateKeyPath": "/path/to/.ssh/id_rsa",
    "remotePath": "/work_dir",
    "uploadOnSave": true,
    "syncMode": "update",
    "watcher":{
        "files": "**/*",
        "autoUpload": false,
        "autoDelete": true
    },
    "ignore":[
        "**/.vs",
        "**/.vscode",
        "**/.DS_Store",
        "**/.git",
        "**/.gitignore",
        "**/__pycache__"
    ] 
}
